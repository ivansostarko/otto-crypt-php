{
  "name": "ivansostarko/otto-crypt-php",
  "description": "A PHP package for Otto Crypt encryption",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Sostarko",
      "email": "ivan.sostarko@hotmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-openssl": "*",
    "ext-sodium": "*",
    "illuminate/support": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "IvanSostarko\\OttoCrypt\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "IvanSostarko\\OttoCrypt\\OttoCryptServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0"
  }
}